	measure 'Variacao Dinamica %' = ```
			
			VAR _IndexSelecionado = SELECTEDVALUE('Parâmetro 2'[Parâmetro Pedido])
			
			VAR _Dias = 
			    SWITCH( _IndexSelecionado,
			        0, 7,   -- Ultimos_7_Dias
			        1, 14,  -- Ultimos_14_Dias
			        2, 30,  -- Ultimos_30_Dias
			        3, 60,  -- Ultimos_60_Dias
			        4, 90,  -- Ultimos_90_Dias
			        5, 180, -- Ultimos_180_Dias
			        6, 365, -- Ultimo_Ano
			        7, 0    -- Todo Periodo
			    )
			
			VAR _DataMaxPeriodo = 
			    CALCULATE(
			        MAX(vw_Calendario_Dinamico[Data]),
			        SWITCH( _IndexSelecionado,
			            0, vw_Calendario_Dinamico[Ultimos_7_Dias] = "Sim",
			            1, vw_Calendario_Dinamico[Ultimos_14_Dias] = "Sim",
			            2, vw_Calendario_Dinamico[Ultimos_30_Dias] = "Sim",
			            3, vw_Calendario_Dinamico[Ultimos_60_Dias] = "Sim",
			            4, vw_Calendario_Dinamico[Ultimos_90_Dias] = "Sim",
			            5, vw_Calendario_Dinamico[Ultimos_180_Dias] = "Sim",
			            6, vw_Calendario_Dinamico[Ultimo_Ano] = "Sim",
			            7, MAX(vw_Calendario_Dinamico[Data]) 
			        ),
			        REMOVEFILTERS(vw_Calendario_Dinamico)
			    )
			
			VAR _Atual = 
			    CALCULATE(
			        [Preco Selecionado],
			        DATESINPERIOD(
			            vw_Calendario_Dinamico[Data], 
			            _DataMaxPeriodo, 
			            -_Dias, 
			            DAY
			        )
			    )
			
			VAR _Anterior = 
			    CALCULATE(
			        [Preco Selecionado],
			        DATESINPERIOD(
			            vw_Calendario_Dinamico[Data], 
			            _DataMaxPeriodo - _Dias, 
			            -_Dias, 
			            DAY
			        ),
			        REMOVEFILTERS(vw_Calendario_Dinamico) 
			    )
			
			RETURN
			    IF(
			        _Dias > 0 && NOT ISBLANK(_Anterior),
			        DIVIDE(_Atual - _Anterior, _Anterior)
			    )
			```
